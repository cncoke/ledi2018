<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ContactItemView">
        <attr name="contactItemImage" format="reference" />
        <attr name="contactItemName" format="string" />
    </declare-styleable>
    <declare-styleable name="TitleBar">
        <attr name="titleBarTitle" format="string" />
        <attr name="titleBarLeftImage" format="reference" />
        <attr name="titleBarLeftText" format="string" />
        <attr name="titleBarRightImage" format="reference" />
        <attr name="titleBarRightText" format="string" />
        <attr name="titleBarBackground" format="reference|color" />
        <attr name="titleBarLeftShow" format="boolean" />
        <attr name="titleBarRightShow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="radius" format="integer" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="MultiLineRadioGroup">
        <attr name="child_margin_horizontal" format="dimension" />
        <attr name="child_margin_vertical" format="dimension" />
        <attr name="child_layout" format="integer" />
        <attr name="child_count" format="integer" />
        <attr name="child_values" format="integer" />
        <attr name="single_choice" format="boolean" />
        <attr name="gravity" format="integer" />
    </declare-styleable>

</resources>